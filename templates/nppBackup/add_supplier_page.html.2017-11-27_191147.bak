<!DOCTYPE html>
<html>
<head>
	<title>Add New Supplier</title>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
	<script src="static/js/add_product_page.js"></script> 
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
   <script src="static/js/w3.js"></script>
</head>
<body>
<div w3-include-html={{ url_for('header') }} ></div>
	<form role="form" action="{{url_for('add_supplier') }}" method="POST">
		<div class="form-group">
		<label> Agency Name:<input type="text" name="name" /></label>
		</div>
		<div class="form-group">
		<label>Supplier name<input type="text" name="contact" /></label>
		</div>
		<div class="form-group">
		<label>Mobile Number<input type="number" name="mobile_no" /></label>
		</div>
		<div class="form-group">
		<label>Address<textarea name="address" ></textarea></label>
		</div>
		<div class="form-group">
		<input type="submit" value="ADD SUPPLIER">
		</div>
	</form>
	
		<div w3-include-html={{ url_for('footer') }}></div>
		<script>
		w3.includeHTML();
		</script>
</body>
</html>