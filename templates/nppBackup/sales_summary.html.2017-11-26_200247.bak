<!DOCTYPE html>
<html>
<head>
	<title>monthly_sales_test</title>
		<style>
	.container {
    width: 300px;
    border: 4px solid grey;
    box-sizing: border-box;
	}
	</style>
	  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <script src="static/js/w3.js"></script>
</head>
<body>
<div w3-include-html={{ url_for('header') }}></div>
	<div class="inner_content_w3_agile_info two_in">
		<h2 class="w3_inner_tittle" style="color:#fff"></h2>
		<input type = "button" onclick="location.href='';" name="week" class = "btn btn-success"  value="Weekly Sales"></input>
		<input type = "button" onclick="calc(event)" name="month" class = "btn btn-success"  value="Monthly Sales"></input>
		<input type = "button" onclick="calc(event)" name="year" class = "btn btn-success"  value="Yearly Sales"></input>
			<div class="forms-main_agileits">
				<div class="forms-inner">
					<div class="wthree_general graph-form agile_info_shadow ">
						<div class="grid-1 ">
							<div class="form-body">
								<div class="container">
									<div class="col-sm-12"><h3 class="text-muted"><mark>Date : {{ start_date }} to {{ end_date }}</mark></h3></div>
										<table class="table table-striped">
										<tr><td>Item</td><td>Total Quantity Sold</td><td>price</td> <td>Total</td></tr>
										{% for i in p %}
											<tr><td>{{ p[i][0] }}</td><td> {{ p[i][1] }}</td><td>{{ m[i] }}</td> <td> {{ p[i][1]*m[i] }}</td></tr>
										{% endfor %}
										</table>
								</div>	
							</div>
						</div>																
					</div>
				</div>
			</div> 
	</div>
	<div w3-include-html={{ url_for('footer') }}></div>
<script>
w3.includeHTML();
</script>
</body>
</html>









