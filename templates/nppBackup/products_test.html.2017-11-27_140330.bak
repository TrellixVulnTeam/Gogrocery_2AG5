<!DOCTYPE html>
<html>
<head>
<style>
	#cont .container {
    width: 300px;
    border: 4px solid grey;
    box-sizing: border-box;
}
	</style>
	<meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <script src="static/js/w3.js"></script>
	<title>products_test</title>
</head>
<body>
<div w3-include-html={{ url_for('header') }} ></div>
<div class="container" id="cont">
<div class="col-sm-12"><h3 class="text-muted">Products Details: </h3></div>
<table class="table table-striped">
<tr> <td>Category</td> <td>Product Name</td> <td>Arrival Date</td> <td>Price</td>  </tr>
	{% for product in products %}
		<li> {{ product.p_category }} {{ product.p_name  }} {{ product.product_arrival_date }} {{ product.p_price }} </li>
	{% endfor %}
	<div w3-include-html={{ url_for('footer') }}></div>
<script>
w3.includeHTML();
</script>
</body>
</html>